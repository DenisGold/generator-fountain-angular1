{
  "name": "generator-crm-angularjs",
  "version": "2.1.5",
  "description": "Yeoman CRM generator to scaffold a webapp with Angular 1 written in ES6 (Babel), TypeScript through Webpack or SystemJS including tools Gulp 4, ESLint, Browsersync and Karma",
  "homepage": "https://indeema.com/",
  "repository": "DenisGold/generator-fountain-angular1",
  "author": "Denis Stolairchuk",
  "contributors": [
    "Denis Stoliarchuk <denis.stolairchuk@indeema.com> (https://github.com/DenisGold/)"
  ],
  "main": "index.js",
  "keywords": [
    "yeoman-generator",
    "yeoman",
    "generator",
    "gulp",
    "gulpfile",
    "fountain",
    "cli",
    "angular"
  ],
  "dependencies": {
    "fountain-generator": "^1.0.0",
    "generator-fountain-gulp": "^1.0.0",
    "snyk": "^1.316.1"
  },
  "devDependencies": {
    "ava": "^0.18.2",
    "babel-eslint": "^7.1.1",
    "chai": "^3.5.0",
    "chai-spies": "^0.7.1",
    "cross-spawn": "^5.0.1",
    "eslint": "^3.15.0",
    "eslint-config-xo-space": "^0.15.0",
    "eslint-plugin-babel": "^4.0.1",
    "gulp": "gulpjs/gulp#4.0",
    "gulp-eslint": "^3.0.1",
    "gulp-exclude-gitignore": "^1.1.1",
    "gulp-nsp": "^2.4.2",
    "nyc": "^10.1.2"
  },
  "nyc": {
    "include": [
      "generators/**/*.js"
    ],
    "exclude": [
      "generators/**/templates/**"
    ]
  },
  "scripts": {
    "test": "gulp",
    "prepublish": "npm run snyk-protect && gulp prepublish",
    "snyk-protect": "snyk protect"
  },
  "license": "MIT",
  "eslintConfig": {
    "extends": "xo-space/esnext"
  },
  "snyk": true
}
